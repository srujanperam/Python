      "source": [
        "#Give n for factorial\n",
        "num=int(input(\"Give your number \"))\n",
        "k=1\n",
        "i=2\n",
        "while i<=num:\n",
        "  k=k*i\n",
        "  i=i+1\n",
        "print(\"Factorial of the number is\",k)"
      ],
